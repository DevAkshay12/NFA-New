  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@cap-js/hana": "^1",
    "@sap-cloud-sdk/http-client": "^3.23.0",
    "@sap-cloud-sdk/resilience": "^3.23.0",
    "@sap/cds": "^8",
    "@sap/xssec": "^4.2.7",
    "express": "^4",
    "passport": "^0.7.0"
  },
  "devDependencies": {
    "@cap-js/cds-types": "^0.7.0",
    "@cap-js/sqlite": "^1",
    "@sap/cds-dk": "^8",
    "rimraf": "^5.0.5"
  },
  "scripts": {
    "start": "cds-serve",
    "watch-nfa_form_": "cds watch --open nfa_form_/webapp/index.html?sap-ui-xx-viewCache=false",
    "undeploy": "cf undeploy NFA_Form --delete-services --delete-service-keys --delete-service-brokers",
    "build": "rimraf resources mta_archives && mbt build --mtar archive",
    "deploy": "cf deploy mta_archives/archive.mtar --retries 1"
  },
  "cds": {
    "sql": {
      "native_hana_associations": false
    },
    "requires": {
      "db": "hana",
      "auth": "dummy"
    }
  },
  "sapux": [
    "app/nfa_form_"
  ]
